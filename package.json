{
  "name": "@talkncloud/tcup",
  "description": "A cli tool for price AWS price estimation of cloudformation templates, json or yaml including CDK",
  "version": "0.0.44",
  "main": "lib/index.js",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "author": "Mike Jacobsson @talkncloud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talkncloud/tnc-cup-client/issues"
  },
  "keywords": [
    "cloud",
    "aws",
    "cloudformation",
    "cdk",
    "talkncloud"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tcup",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.",
      "registry": "https://www.npmjs.com/package/@talkncloud/tcup"
    }
  },
  "homepage": "https://cup.talkncloud.com",
  "repository": "https://github.com/talkncloud/tnc-cup-client",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.6",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.25",
    "@types/strip-color": "^0.1.0",
    "chai": "^4.3.0",
    "eslint": "^7.19.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.2",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/js-yaml": "^4.0.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
    "cli-ux": "^5.5.1",
    "cloudformation-js-yaml-schema": "^0.4.2",
    "enquirer": "^2.3.6",
    "fs-extra": "^9.1.0",
    "js-yaml": "^4.0.0",
    "strip-color": "^0.1.0",
    "tslib": "^2.1.0"
  },
  "bin": {
    "tcup": "./bin/run"
  }
}
